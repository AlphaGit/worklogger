    "googleapis": "^30.0.0",
    "log4js": "^2.6.0",
    "minimist": "^1.2.0",
    "moment": "^2.22.1",
    "node-fetch": "^1.7.3"
  },
  "devDependencies": {
    "eslint": "^4.14.0",
