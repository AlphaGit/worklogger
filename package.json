{
  "name": "worklogger",
  "version": "1.2.0",
  "description": "Reads worklogs from a variety of sources and consolidates them to several outputs.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlphaGit/worklogger.git"
  },
  "scripts": {
    "start": "node index.js",
    "cover": "nyc --include='app/**/*.ts' yarn test",
    "test:all": "npm run lint && npm run cover",
    "test": "jest",
    "lint": "eslint --format table '**/*.ts'",
    "lint:fix": "eslint --fix .",
    "watch:test": "watch \"npm test\" app tests",
    "watch:all": "watch \"npm run test:all\" app tests"
  },
  "keywords": [
    "work",
    "logger",
    "worklog",
    "harvest",
    "jira",
    "tempo"
  ],
  "author": "Alpha <alphagma@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlphaGit/worklogger/issues"
  },
  "homepage": "https://github.com/AlphaGit/worklogger#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.11.0",
    "app-module-path": "^2.1.0",
    "aws-sdk": "^2.751.0",
    "fetch-event": "^1.0.0-alpha",
    "googleapis": "^59.0.0",
    "log4js": "^6.3.0",
    "minimist": "^1.2.3",
    "moment-timezone": "^0.5.32",
    "mustache": "^4.1.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/log4js": "^2.3.5",
    "@types/mocha": "^8.2.0",
    "@types/node-fetch": "^2.5.9",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "serverless": "^2.16.0",
    "serverless-event-constant-inputs": "0.0.1",
    "serverless-offline": "^6.8.0",
    "sinon": "^9.2.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "watch": "^1.0.2"
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "preset": "ts-jest",
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    }
  }
}
